"
" dont want to expand tab to spaces
" set expandtab

" more info here
" https://linuxhandbook.com/vim-indentation-tab-spaces
" https://vi.stackexchange.com/a/28017

set autoindent
set tabstop=2

set shiftwidth=2
set softtabstop=2

set noexpandtab

syntax on
colorscheme delek

set number
" set mouse=a

set hlsearch
